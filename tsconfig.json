{
  "compilerOptions": {
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "bundler",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "incremental": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "target": "ES6",
      "plugins": [
          {
              "name": "next"
          }
      ],
      "paths": {
          "@/*": ["./src/*"]
      }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/controllers/libs/google-auth.js", "src/components/common/HeartCursor/HeartCursor.tsx"],
  "exclude": ["node_modules"]
}
